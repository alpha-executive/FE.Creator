  </configSections>
  <connectionStrings>
    <!--<add name="mssqlconnection" connectionString="Data Source=.;Initial Catalog=ObjectRepository;Integrated Security=SSPI;" providerName="System.Data.SqlClient"/>-->
    <add name="mssqlconnection" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\ObjectRepository.mdf;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="sqliteconnection" connectionString="Data Source=|DataDirectory|\ObjectRepository.sqlite" providerName="System.Data.SQLite.EF6" />
    <add name="localfileindex" connectionString="Data Source=|DataDirectory|\localfileindex.sqlite" providerName="System.Data.SQLite.EF6" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="vs:EnableBrowserLink" value="false" />

    <!--External login Client Ids (OAuth2)-->
    <!--Microsoft-->
    <add key="microsoftClientId" value="{EE390F5A-D033-480C-B789-52D024D94290}" />
    <add key="microsoftSecurityKey" value="{EE390F5A-D033-480C-B789-52D024D94290}" />
    
    <!--Google-->
    <add key="googleClientId" value="{DB768319-06A4-4976-8A61-DA310037D043}" />
    <add key="googleSecurityKey" value="{EE390F5A-D033-480C-B789-52D024D94290}" />

    <!--sqlite or mssqlserver, mysql is not work well-->
    <add key="databaseprovider" value="sqlite" />

    <add key="IsDataForLoginUserOnly" value="true"/>

    <add key="SuperAdmin" value="win@2018.com"/>
  </appSettings>
  <!--
