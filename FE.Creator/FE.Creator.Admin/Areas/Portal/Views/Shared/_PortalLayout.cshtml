<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8">
    <title>FE-Portal | Share Content to Our Public</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <link rel="shortcut icon" href="favicon.ico">

    <!-- CSS -->
    <link href="/Content/portal/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/portal/css/flexslider.css" rel="stylesheet" type="text/css" />
    <link href="/Content/portal/css/animate.css" rel="stylesheet" type="text/css" media="all" />
    <link href="/Content/portal/css/angular-bootstrap-lightbox.css" rel="stylesheet" type="text/css" />


    <!-- FONTS -->
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500italic,700,500,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <link href="http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">

    <link rel="stylesheet" href="~/Content/portal/css/share.min.css">
    <link href="/Content/portal/css/style.css" rel="stylesheet" type="text/css" />
    <!-- SCRIPTS -->
    <!--[if IE]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
    <!--[if IE]><html class="ie" lang="en"> <![endif]-->

    <script src="/Content/portal/js/jquery.min.js" type="text/javascript"></script>
    <script src="/Content/portal/js/bootstrap.min.js" type="text/javascript"></script>
    @*<script src="/Content/portal/js/jquery.prettyPhoto.js" type="text/javascript"></script>*@
    <script src="/Content/portal/js/jquery.nicescroll.min.js" type="text/javascript"></script>
    <script src="/Content/portal/js/superfish.min.js" type="text/javascript"></script>
    <script src="/Content/portal/js/jquery.flexslider-min.js" type="text/javascript"></script>
    @*<script src="/Content/portal/js/owl.carousel.js" type="text/javascript"></script>*@
    @*<script src="/Content/portal/js/animate.js" type="text/javascript"></script>*@
    <script src="/Content/portal/js/jquery.BlackAndWhite.js"></script>
    <script src="/Content/portal/js/bwportal.js" type="text/javascript"></script>
   
  

    @Scripts.Render("~/js/angularjs")
    @Scripts.Render("~/js/angularroute")
    @Scripts.Render("~/js/angularmessage")
    @Scripts.Render("~/js/angularselect")
    @Scripts.Render("~/js/angularupload")
    @Scripts.Render("~/js/adminapp/objectrepository")
    @Scripts.Render("~/js/adminapp/dataservice")
    @Scripts.Render("~/js/adminapp/pageservice")
    @Scripts.Render("~/js/adminapp/objectutilservice")
    <script src="/Content/portal/js/angular-animate.js" type="text/javascript"></script>
    <script src="/Content/portal/js/ui-bootstrap-tpls-1.3.2.js" type="text/javascript"></script>
    <script src="/Content/portal/js/angular-bootstrap-lightbox.js" type="text/javascript"></script>
    <script src="~/Content/portal/js/portalentry.js" type="text/javascript"></script>
    <script src="~/Content/portal/js/portalcontrollers.js" type="text/javascript"></script>

</head>
<body>
    <!-- PRELOADER -->
    <img id="preloader" src="/Content/portal/images/preloader.gif" alt="" />
    <!-- //PRELOADER -->
    <div class="preloader_hide">
        <!-- PAGE -->
        <div id="page" ng-app="fePortal">
            @RenderBody()
        </div><!-- //PAGE -->
        <!-- CONTACTS -->
        <section id="contacts"></section><!-- //CONTACTS -->
        <!-- FOOTER -->
        <footer>

            <!-- CONTAINER -->
            <div class="container">

                <!-- ROW -->
                <div class="row" data-appear-top-offset="-200" data-animated="fadeInUp">

                    <div class="col-lg-4 col-md-4 col-sm-6 padbot30">
                        <h4><b>@FE.Creator.Admin.lang.ConsolePortal.PORTAL_FOOTER_DONATE</b></h4>
                        <div>@FE.Creator.Admin.lang.ConsolePortal.PORTAL_FOOTER_DONATE_DESC</div>
                        <div><a href="#">@FE.Creator.Admin.lang.ConsolePortal.PORTAL_FOOTER_DONATE_LINK</a></div>
                    </div>

                    <div class="col-lg-4 col-md-4 col-sm-6 padbot30 foot_about_block">
                        <h4><b>@FE.Creator.Admin.lang.ConsolePortal.PORTAL_FOOTER_ABOUT</b></h4>
                        <p>@FE.Creator.Admin.lang.ConsolePortal.PORTAL_FOOTER_ABOUT_DESC</p>
                        <h4><b>@FE.Creator.Admin.lang.ConsolePortal.PORTAL_FOOTER_LICENSE</b></h4>
                        <p>@FE.Creator.Admin.lang.ConsolePortal.PORTAL_FOOTER_LICENSE_DESC</p>
                    </div>

                    <div class="respond_clear"></div>

                    <div class="col-lg-4 col-md-4 padbot30">
                        <h4><b>@FE.Creator.Admin.lang.ConsolePortal.PORTAL_FOOTER_CONTACTUS</b></h4>
                        <ul class="contacts-list">
                            <li><i class="fa fa-envelope-o"></i> alpha_executive@126.com</li>
                            <li><i class="map_show fa fa-map-marker"></i> @FE.Creator.Admin.lang.ConsolePortal.PORTAL_FOOTER_CONTACT_ADDR</li>
                        </ul>
                    </div>
                </div><!-- //ROW -->
            </div><!-- //CONTAINER -->
        </footer><!-- //FOOTER -->
    </div>
</body>
</html>
