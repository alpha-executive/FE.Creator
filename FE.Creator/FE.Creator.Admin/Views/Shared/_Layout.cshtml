<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title - Admin Console</title>
    @Styles.Render("~/css/adminlte")
    @Styles.Render("~/css/bootstrap-weather")   
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    @Scripts.Render("~/bundles/modernizr")
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    @Scripts.Render("~/bundles/respond")
    <![endif]-->

    @Scripts.Render("~/js/adminlte")
    @Scripts.Render("~/js/ckeditor")
    @Scripts.Render("~/js/ckeditor-jquery")
    @Scripts.Render("~/js/angularjs")
    @Scripts.Render("~/js/angularroute")
    @Scripts.Render("~/js/angularmessage")
    @Scripts.Render("~/js/angularselect")
    @Scripts.Render("~/js/angularupload")
    @Scripts.Render("~/js/adminapp/objectrepository")
    @Scripts.Render("~/js/adminapp/dataservice")
    @Scripts.Render("~/js/adminapp/pageservice")
    @Scripts.Render("~/js/adminapp/objectutilservice")
    @Scripts.Render("~/js/adminapp/usersimpleprofile")
</head>
<body class="hold-transition skin-blue-light sidebar-mini sidebar-collapse">
  
    <!-- Site wrapper -->
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="/" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>FE</b>A</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>FE</b>Console</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        <li class="dropdown messages-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-envelope-o"></i>
                                <span class="label label-success">4</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">You have 4 messages</li>
                                <li>
                                    <!-- inner menu: contains the actual data -->
                                    <ul class="menu">
                                        <li>
                                            <!-- start message -->
                                            <a href="#">
                                                <div class="pull-left">
                                                    <img src="@Url.Content("~/Content/adminlte-2.3.6/dist/img/user2-160x160.jpg")" class="img-circle" alt="User Image">
                                                </div>
                                                <h4>
                                                    Support Team
                                                    <small><i class="fa fa-clock-o"></i> 5 mins</small>
                                                </h4>
                                                <p>Why not buy a new awesome theme?</p>
                                            </a>
                                        </li>
                                        <!-- end message -->
                                    </ul>
                                </li>
                                <li class="footer"><a href="#">See All Messages</a></li>
                            </ul>
                        </li>
                        <!-- Notifications: style can be found in dropdown.less -->
                        <li class="dropdown notifications-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-bell-o"></i>
                                <span class="label label-warning">10</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">You have 10 notifications</li>
                                <li>
                                    <!-- inner menu: contains the actual data -->
                                    <ul class="menu">
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-users text-aqua"></i> 5 new members joined today
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                                <li class="footer"><a href="#">View all</a></li>
                            </ul>
                        </li>
                        <!-- Tasks: style can be found in dropdown.less -->
                        <li id="tasks_on_menu" class="dropdown tasks-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-flag-o"></i>
                                <span class="label label-danger taskcount">0</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">You have no tasks</li>
                                <li>
                                    <!-- inner menu: contains the actual data -->
                                    <ul class="menu">
                                    </ul>
                                </li>
                                <li class="footer">
                                    <a href="/ngview/Index/Efficiency/TaskIndex">View all tasks</a>
                                </li>
                            </ul>
                        </li>
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Content/apps/img/userdefault.png" class="user-image img-curr-login" alt="User Image">
                                <span class="hidden-xs login-user-name">@Request.RequestContext.HttpContext.User.Identity.Name</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="~/Content/apps/img/userdefault.png" class="img-circle img-curr-login" alt="User Image">
                                    <p>
                                        <span class="login-user-name">@Request.RequestContext.HttpContext.User.Identity.Name</span>
                                        <small id="loginUserBirthDate"></small>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="#" class="btn btn-default btn-flat" id="loginUserProfileLink">Profile</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="/Account/LogOff" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>

                        </li>
                        <!-- Control Sidebar Toggle Button -->
                        <li>
                            <a href="/ngview/Index/Settings/AppSettingsIndex"><i class="fa fa-gears"></i></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- =============================================== -->
        <!-- Left side column. contains the sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="@Url.Content("~/Content/apps/img/userdefault.png")" class="img-circle img-curr-login" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p class="login-user-name">@Request.RequestContext.HttpContext.User.Identity.Name</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header">MAIN NAVIGATION</li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-cogs"></i> <span>System Configuraion</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/ngview/Index/ObjectRepository/ObjectDefinitionGroupIndex"><i class="fa fa-object-group"></i> Object Group</a></li>
                            <li><a href="/ngview/Index/ObjectRepository/ObjectDefinitionIndex"><i class="fa fa-table"></i>Object Schema</a></li>
                            <li><a href="/ngview/Index/LicenseManagement/LicenseIndex"><i class="fa fa-balance-scale"></i>License Management</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-diamond"></i>
                            <span>Data Management</span>
                            <span class="pull-right-container">
                                <small class="label pull-right bg-red">Hot</small>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/ngview/Index/ObjectRepository/GeneralObjectIndex"><i class="fa fa-opera"></i> General Objects</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="/ngview/Index/Documents/DocumentIndex">
                            <i class="fa fa-folder"></i>
                            <span>Documents</span>
                        </a>
                    </li>
                    <li>
                        <a href="/ngview/Index/Books/BookIndex">
                            <i class="fa fa-book"></i>
                            <span>Books</span>
                        </a>
                    </li>
                    <li>
                        <a href="/ngview/Index/Photos/PhotoIndex">
                            <i class="fa fa-image"></i>
                            <span>Photos</span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-rocket"></i> <span>Efficiency</span>
                            <span class="pull-right-container">
                                <small class="label pull-right bg-yellow"><i class="fa fa-heart-o"></i></small>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/ngview/Index/Efficiency/TaskIndex"><i class="fa fa-tasks"></i> Task Management</a></li>
                            <li><a href="/ngview/Index/Efficiency/DiaryIndex"><i class="fa fa-edit"></i>Diary</a></li>
                            <li><a href="/ngview/Index/Efficiency/ArticleIndex"><i class="fa fa-lightbulb-o"></i>Posts</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="/ngview/Index/Contacts/ContactsIndex">
                            <i class="fa fa-phone-square"></i>
                            <span>Contacts</span>
                        </a>
                    </li>
                    <li>
                        <a href="/ngview/Index/AccountManagement/AccountIndex">
                            <i class="fa fa-expeditedssl"></i>
                            <span>Account Management</span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-users"></i> <span>User Management</span>
                            <span class="pull-right-container">
                                <small class="label pull-right bg-green">New</small>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/ngview/Index/SystemUsers/UsersIndex"><i class="fa fa-opera"></i> System Users</a></li>
                        </ul>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @ViewBag.Title - Admin Console
                    <small>Just for the Best Experience!</small>
                </h1>
            </section>
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0.0.0
            </div>
            <strong>Copyright &copy; 2016 <a href="#">FE Contributes Space</a>.</strong> All rights reserved.
        </footer>
    </div>
        @RenderSection("scripts", required: false)
        <script type="text/javascript">
	        // To make Pace works on Ajax calls
	        $(document).ajaxStart(function() { Pace.restart(); });
        </script>
</body>
</html>
