@model int
<div class="row" ng-app="ngObjectRepository" ng-init="grpCtrl.Initialzie()" ng-controller="editObjectDefinitionGroupController as grpCtrl">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">Object Definition Group</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form role="form">
                <div class="box-body">
                    <div class="form-group">
                        <label for="groupName">Group Name</label>
                        <input class="form-control" id="groupName" type="text" ng-model="grpCtrl.currentGroup.groupName" placeholder="Enter Group Name">
                    </div>
                    <div class="form-group">
                        <label for="groupKey">Group Key</label>
                        <input class="form-control" id="groupKey" type="text" ng-model="grpCtrl.currentGroup.groupKey" placeholder="Enter Group Key">
                    </div>
                </div>
                <!-- /.box-body -->
                <div class="box-footer">
                    <button class="btn btn-primary" type="submit" ng-click="UpdateGroup()">Submit</button>
                </div>
            </form>
        </div>
</div>