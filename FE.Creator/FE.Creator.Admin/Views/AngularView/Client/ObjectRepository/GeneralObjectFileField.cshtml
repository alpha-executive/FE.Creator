<div class="col-md-12">
    <div class="box">
        <div class="box-header">
            <h5 class="box-title"><i class="fa fa-paperclip"></i> {{objfield.keyName}}</h5>
        </div>
        <div class="box-body">
            <div class="row">
                <div class="col-md-6">
                    ngf-max-height="1000" ngf-max-size="10MB">
                    <button class="btn btn-warning" type="file" ngf-select="ctrl.uploadFiles($file, $invalidFiles,objfield)"
                            ngf-max-height="1000" ngf-max-size="10MB">
                       <i class="fa fa-upload"></i> Upload File
                    </button>
                    <div style="font:smaller">
                        {{ctrl.f.name}} {{ctrl.errFile.name}} {{ctrl.errFile.$error}} {{ctrl.errFile.$errorParam}}
                        <span class="progress" ng-show="ctrl.f.progress >= 0">
                            <div style="width:{{ctrl.f.progress}}%"
                                 ng-bind="ctrl.f.progress + '%'"></div>
                        </span>
                    </div>
                    {{ctrl.errorMsg}}
                </div>
                <div class="col-md-6">
                    <ul class="mailbox-attachments clearfix">
                        <li>
                            <span class="mailbox-attachment-icon"><i class="fa fa-file-pdf-o"></i></span>
                            <div class="mailbox-attachment-info">
                                <a href="{{objfield.value.fileUrl}}" class="mailbox-attachment-name"><i class="fa fa-paperclip"></i> {{ objfield.value.fileName }}</a>
                                <span class="mailbox-attachment-size">
                                    {{objfield.value.fileSize}} KB
                                    <a href="#" class="btn btn-default btn-xs pull-right"><i class="fa fa-cloud-download"></i></a>
                                </span>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
