       
<div class="alert alert-dismissible" ng-class="CurrMessageClass" ng-show="ShowWarningMsg">
    <button class="close" aria-hidden="true" type="button" ng-click="ShowWarningMsg=false">×</button>
    <h4><i class="icon fa fa-warning"></i> @FE.Creator.Admin.lang.AppLang.DATA_CUST_UPDATE_RESULTS</h4>
    {{UpdateMessage}}
</div> 
       <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">@FE.Creator.Admin.lang.AppLang.DATA_CUST_TABLEGROUP</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form role="form">
                <div class="box-body">
                    <div class="form-group">
                        <label for="groupName">@FE.Creator.Admin.lang.AppLang.DATA_CUST_TABLE_GRPNAME</label>
                        <input class="form-control" id="groupName" type="text" ng-model="currentGroup.groupName" placeholder="Enter Group Name">
                    </div>
                    <div class="form-group">
                        <label for="groupKey">@FE.Creator.Admin.lang.AppLang.DATA_CUST_TABLE_GRPKEY</label>
                        <input class="form-control" id="groupKey" type="text" ng-model="currentGroup.groupKey" placeholder="Enter Group Key">
                    </div>
                </div>
                <!-- /.box-body -->
                <div class="box-footer">
                    <button class="btn btn-primary" type="submit" ng-click="SaveChange()">@FE.Creator.Admin.lang.AppLang.COMMON_BTN_SAVE</button>
                    <a class="btn btn-primary" href="/defgrps">@FE.Creator.Admin.lang.AppLang.CONTACTS_BTN_RETURNTOLIST</a>
                </div>
            </form>
        </div>