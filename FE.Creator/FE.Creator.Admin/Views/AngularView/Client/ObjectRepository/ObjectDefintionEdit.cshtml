<div class="col-lg-12">
    <div class="box box-success box-solid">
        <div class="box-header with-border">
            <h3 class="box-title"><i class="fa fa-table"></i> @FE.Creator.Admin.lang.AppLang.DATA_CUST_EDITTABLE - {{CurrentObjectDefinition.objectDefinitionName}}</h3>
            <div class="box-tools pull-right">
                <div class="btn-group">
                    <button type="button" class="btn btn-box-tool" ng-click="saveChanges()"><i class="fa fa-save"></i> @FE.Creator.Admin.lang.AppLang.COMMON_BTN_SAVE</button>
                    <a class="btn btn-box-tool" href="/objdefs/{{groupId}}"><i class="fa fa-mail-reply"></i> @FE.Creator.Admin.lang.AppLang.DATA_CUST_BACKTOLIST</a>
                </div>
            </div>
        </div>
        <div class="box-body">
            <div class="box box-warning">
                <div class="box-header">
                    <h4><i class="fa fa-flag-o"></i> @FE.Creator.Admin.lang.AppLang.DATA_CUST_GENERAL_PROPERTY</h4>
                </div>
                <div class="box-body">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-star-o text-yellow"></i></span>
                        <input type="text" class="form-control" placeholder="@FE.Creator.Admin.lang.AppLang.CUST_DATA_TABLE_NAME" ng-model="CurrentObjectDefinition.objectDefinitionName">
                    </div>
                    <br />
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-star-o text-yellow"></i></span>
                        <input type="text" class="form-control" placeholder="@FE.Creator.Admin.lang.AppLang.CUST_DATA_TABLE_KEY" ng-model="CurrentObjectDefinition.objectDefinitionKey">
                    </div>
                </div>
            </div>
            <div class="box box-warning">
                <div class="box-header">
                    <h4 class="box-title"><i class="fa fa-cubes"></i> @FE.Creator.Admin.lang.AppLang.DATA_CUST_ADVANCED_PROPERTY</h4>
                    <div class="box-tools pull-right">
                        <div class="btn-group">
                            <span type="button" class="btn btn-warning btn-box-tool"><i class="fa fa-plus"></i> @FE.Creator.Admin.lang.AppLang.COMMON_BTN_ADD</span>
                            <button type="button" class="btn btn-warning btn-box-tool dropdown-toggle" data-toggle="dropdown">
                                <span class="caret"></span>
                                <span class="sr-only"> @FE.Creator.Admin.lang.AppLang.DATA_CUST_TOGGLE_DROPDOWN</span>
                            </button>
                            <ul class="dropdown-menu" role="menu">
                                <li><a ng-click="addPrimaryField(0)"> @FE.Creator.Admin.lang.AppLang.DATA_CUST_FIELD_STRING</a></li>
                                <li><a ng-click="addPrimaryField(1)"> @FE.Creator.Admin.lang.AppLang.DATA_CUST_FIELD_INTEGER</a></li>
                                <li><a ng-click="addPrimaryField(2)"> @FE.Creator.Admin.lang.AppLang.DATA_CUST_FIELD_BIGINTEGER</a></li>
                                <li><a ng-click="addPrimaryField(3)"> @FE.Creator.Admin.lang.AppLang.DATA_CUST_FIELD_DATETIME</a></li>
                                <li><a ng-click="addPrimaryField(4)"> @FE.Creator.Admin.lang.AppLang.DATA_CUST_FIELD_NUMBER</a></li>
                                @*<li><a ng-click="addPrimaryField(5)">Binary Field</a></li>*@
                                <li><a ng-click="addFileUploadField()">@FE.Creator.Admin.lang.AppLang.DATA_CUST_FIELD_FILE</a></li>
                                <li><a ng-click="addObjectReferenceField()">@FE.Creator.Admin.lang.AppLang.DATA_CUST_FIELD_REFERNCE</a></li>
                                <li><a ng-click="addSingleSelectionField()">@FE.Creator.Admin.lang.AppLang.DATA_CUST_FIELD_SINGLE_CHOICE</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="box-body">
                    <div class="box-group" id="accordion">
                        <!-- we are adding the .panel class so bootstrap.js collapse plugin detects it -->
                        <div class="panel box box-default" ng-repeat="fld in CurrentObjectDefinition.objectFields">
                            <div class="box-header with-border">
                                <h4 class="box-title">
                                    <span data-toggle="collapse" style="cursor:pointer;" data-parent="#accordion" href="#collapseItem_{{$index}}" aria-expanded="{{$first}}">
                                        {{fld.objectDefinitionFieldName}}
                                    </span>
                                </h4>
                                <div class="box-tools pull-right">
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-box-tool" ng-click="deleteObjectDefinitionField(fld)"><i class="fa fa-trash"></i> @FE.Creator.Admin.lang.AppLang.COMMON_BTN_DELETE</button>
                                    </div>
                                </div>
                            </div>
                            <div id="collapseItem_{{$index}}" class="panel-collapse collapse {{$first? 'in' : ''}}" aria-expanded="{{$first}}">
                                <div class="box-body">
                                    <div ng-include="getFieldTemplateUrl(fld.generalObjectDefinitionFiledType)"></div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- /.box-body -->
            </div><!--/.box-->
        </div><!-- /.box-body -->
        <div class="box-footer">
            <div class="box-tools pull-right">
                <div class="btn-group">
                    <button type="button" class="btn btn-box-tool" ng-click="saveChanges()"><i class="fa fa-save"></i> @FE.Creator.Admin.lang.AppLang.COMMON_BTN_SAVE</button>
                    <a class="btn btn-box-tool" href="/objdefs/{{groupId}}"><i class="fa fa-mail-reply"></i> @FE.Creator.Admin.lang.AppLang.DATA_CUST_BACKTOLIST</a>
                </div>
            </div>
        </div>
    </div>
</div>